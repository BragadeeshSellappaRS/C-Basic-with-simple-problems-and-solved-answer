// Triangle problem
/**
int main(){
  int a1,a2,a3;
  printf("Enter the 1st angle: ");
  scanf("%d",&a1);
  printf("Enter the 2nd angle: ");
  scanf("%d",&a2);
  printf("Enter the 3rd angle: ");
  scanf("%d",&a3);
  int total = a1+a2+a3;
  if(total == 180){
    //Triangle
    if(a1==90 || a2==90 || a3 == 90){
      printf("Right angle");
    }
    else if(a1==a2==a3){
      printf("Acute angle");
    }
    else if(a1> 90 || a2 >90 || a3 > 90){
      printf("Obtuse angle");

    }
  }
}
**/
/**

//Quadratic Problems
int main(){
  int a,b,c,root1,root2;
  printf("Enter the value a: ");
  scanf("%d",&a);
  printf("Enter the value b: ");
  scanf("%d",&b);
  printf("Enter the value: c: ");
  scanf("%d",&c);
  int ans;
  ans = b*b - 4*a*c;
  if(ans > 0){
    root1 = (-b * sqrt(ans))/(2*a*c);
    root2 = (b * sqrt(ans))/(2*a*c);
    printf("Root1 : %f\n Root2: %f",root1,root2);
  }

}
**/

// Decimal to Binary 1 count
/**
int main(){
  int count = 0,n=14;
  while(n){
    if(n & 1){
      count++;
    }
    n >>= 1;
  }
  printf("%d is count of 1s in %d",count,n);
}**/

// Count number of digits
/**
int main(){
  int a = 258,count=0;
  while(a != 0){
    a = a/10;
    count++;
  }
  printf("%d",count);
}**/

// Reverse number
/**
int main() {
  int a = 123,b=0;
  while(a!=0){
    int c= a%10;
    b = b*10 + c;
    a /= 10;

  }
   printf("%d", b);
}**/

// Fibonacci series
/**
int main(){
  int range = 10,prev=0,cur=1;
  printf("%d %d ",prev,cur);
  int n = cur+prev;
  for(int i=0;i<range;i++){
    prev = cur;
    cur = n;
    n = cur + prev;
    printf("%d ",cur);
  }
}**/

// Palindrome
/**
int main(){
  int a=101,rev;
  int ori = a;
   while(a!=0){
     int c= a%10;
     rev = rev*10 + c;
     a /= 10;
   }
  if(ori==rev){
    printf("Palindrome");
  }else{
    printf("Not Palindrome");
  }
}**/

