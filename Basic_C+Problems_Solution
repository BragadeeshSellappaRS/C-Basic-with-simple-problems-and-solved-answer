//Double Triangle pattern
/*
int main(){
  int n = 5;
  for(int i=0;i<=n;i++){
    for(int j=0;j<i;j++){
      printf("* ");
    }
    printf("\n");
  }
  for(int i=n;i>=1;i--){
    for(int j=0;j<i;j++){
      printf("* ");
    }
    printf("\n");
  }
}*/


// Pyramid
/*
int main() {
  int n = 5;
  for(int i=0;i<=n-1;i++){
    for(int space = 0;space<=n-i;space++){
      printf(" ");
    }
    for(int j=0;j<=(i*2-1)+1;j++){
      printf("*");
    }
    printf("\n");
  }
}*/

// Reverse Pyramid
/*
int main(){
  int n = 5;
  for(int i=n;i>=1;i--){
    for(int space = 0;space<=n-i;space++){
      printf(" ");
    }
    for(int j=0;j<(i*2-1);j++){
      printf("*");
    }
    printf("\n");
  }
}*/

// Double Pyramid
/*
